# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_BUCKET_NAME=your-bucket-name

# Upload Configuration
NEXT_PUBLIC_MAX_FILE_SIZE=104857600  # 100MB in bytes
NEXT_PUBLIC_MAX_FILES=20             # Maximum number of files
NEXT_PUBLIC_CHUNK_SIZE=5242880       # 5MB chunk size for multipart upload
NEXT_PUBLIC_CONCURRENT_UPLOADS=3     # Number of concurrent uploads

# Application Configuration
NEXT_PUBLIC_BASE_URL=http://localhost:3000